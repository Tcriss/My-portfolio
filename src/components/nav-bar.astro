---
const links: { name: string, path: string }[] = [
  {
    name: 'Home',
    path: '#home'
  },
  {
    name: 'Projects',
    path: '#projects'
  }
]
---
---
<nav id="nav-bar" class="w-full h-20 fixed top-0">
  <section class="w-3"></section>
  <ul class="flex grow w-full h-full justify-center gap-2 items-center">
    <li class="font-mono">
      {
        links && links.map(link => <a href={link.path}>{link.name}</a>)
      }
    </li>
  </ul>
  <section class="w-3"></section>
</nav>
