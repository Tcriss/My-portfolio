---
import Footer from "../components/footer.astro";
import NavBar from "../components/nav-bar.astro";

const pageTitle = Astro.props;
---
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="generator" content={Astro.generator} />
    <title>Cristian Tejeda</title>
</head>
<body class="flex flex-col items-center transition-all duration-500">
    <NavBar/>
    <main class="w-4/6">
        <slot class="grid gap-5"/>
        <div id="separator" class="h-16"></div>
        <Footer/>
    </main>
</body>
</html>